{
    "name": "kentaroutakeda/laravel-safe-routing",
    "license": "MIT",
    "authors": [
        {
            "name": "KentarouTakeda",
            "email": "takeda@youmind.jp"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/yaml": "^3.0|^4.0",
        "laravel/framework": "^6.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "KentarouTakeda\\SafeRouting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KentarouTakeda\\SafeRouting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KentarouTakeda\\SafeRouting\\SafeRoutingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^4.0",
        "spatie/phpunit-watcher": "^1.12"
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit",
        "watch": "phpunit-watcher watch"
    }
}
